<h1> Add New Vehicle</h1>
<form action="/vehicles" method="post">
   <table>
		<tr> <td> Vehicle ID: </td> <td> <input type="number" name="vehicle_id" required> </td> </tr>
		<tr> <td> Vehicle Name: </td> <td> <textarea name="vehicle_name" rows="1" cols="20"> </textarea>  </td> </tr>
		<tr> <td> Vehicle Description: </td> <td> <textarea name="vehicle_description" rows="10" cols="20"> </textarea>  </td> </tr>
		<tr> <td> Vehicle Amount: </td> <td> <input type="number" name="vehicle_amount" required> </td> </tr>
   </table>
   <button type="submit">Save</button>
</form>
